        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".signup.RegisterActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".eventFeed.PeopleGoing" />
        <activity android:name=".login.Profile" />
        <activity android:name=".eventFeed.MainActivity" />
        <activity android:name=".login.ActivityLoginEmail"/>
        <activity android:name=".profiles.Profile_girl" />
        <activity android:name=".profiles.Profile_guy" />
        <activity android:name=".profiles.Profile_guy1" />
        <activity android:name=".eventFeed.InviteSent" />
        <activity android:name=".signup.CreateProfileActivity" />
        <activity android:name=".profiles.UserProfile"/>
        <activity android:name=".profiles.MessagingActivity"/>
        <activity android:name=".profiles.ChatActivity"/>
        <activity android:name=".Chat21"/>


    <meta-data tools:replace="android:value" android:name="com.facebook.sdk.ApplicationId" android:value="@string/facebook_app_id"/>
    </application>


</manifest>